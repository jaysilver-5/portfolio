{
  "version": 2,
  "builds": [
    {
      "src": "build",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "devCommand": "npm run start",
  "outputDirectory": "build",
  "public": true
}
